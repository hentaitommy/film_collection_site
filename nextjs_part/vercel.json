{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://film-collection-site-management.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,DELETE"
        },
        {
          "key": "Vary",
          "value": "Origin"
        }
      ]
    }
  ]
}